<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bitslibrary">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BitsLibrary">
        <activity android:name=".PengaturanAkunActivity"></activity>
        <activity android:name=".PinjamActivity" />
        <activity android:name=".DetailPinjamanActivity" />
        <activity android:name=".KonfirmasiActivity" />
        <activity android:name=".BookActivity" />
        <activity android:name=".AllBookActivity" />
        <activity android:name=".MainActivity" />

        <activity android:name=".DaftarPinjamanActivity"
            android:label="Daftar Pinjam"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>